<li class="search-random-quick-tip">
  <span
    class={{concat-class
      "tip-label"
      (if this.randomTip.clickable "tip-clickable")
    }}
    role="button"
    {{on "click" this.onClick}}
  >
    {{this.randomTip.label}}
  </span>

  <span class="tip-description">
    {{this.randomTip.description}}
  </span>
</li>